@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <meta charset="UTF-8">
    <link href="../../Content/style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap1.css" rel="stylesheet" />
    <link href="../../Content/Site.css" rel="stylesheet" />
    <link href="../../Content/prettyPhoto.css" rel="stylesheet" />
    <link href="../../Content/layout.css" rel="stylesheet" />
    <link href="../../Content/ie_style.css" rel="stylesheet" />
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/cufon-yui.js" type="text/javascript"></script>
    <script src="../../Scripts/cufon-replace.js" type="text/javascript"></script>
    <script type="text/javascript" src="../../Scripts/Josefin_Sans_600.font.js"></script>
    <script type="text/javascript" src="../../Scripts/Lobster_400.font.js"></script>
    <script type="text/javascript" src="../../Scripts/sprites.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery.jplayer.min.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery.jplayer.settings.js"></script>
    <script type="text/javascript" src="../../Scripts/gSlider.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery.prettyPhoto.js"></script>
    <script src="~/Scripts/bootstrap1.js"></script>
    <script>
        $(document).ready(function () {

            $('#kaka').click(function () {
                var value = $('#pizza').val();
                $.getJSON('/Home/search?jalebi=' + value, function (data) {
                    var g = '';
                    $('#myTble').empty();
                    $.each(data, function (index,item)
                    {
                        g=g+'<tr><td>'+item.Name+'</td></tr>'
                    
                    });
                    $(g).appendTo($("#myTble"));
                        
                });
            });
        });
    </script>
</head>
<body>
   
  <nav class="navbar navbar-inverse" role="navigation">
      <div>
       
              <div class="btn-group pull-left" >
                  <button class="btn btn-success" onclick="parent.location='/Home/Index'">Home</button> 
                  <button class="btn btn-success">SignUp</button>
                  <button class="btn btn-success">SignIn</button>
                  <button class="btn btn-success" onclick="parent.location='/Home/Contact'">Contact Us</button>
              </div>
          <div class="navbar-form navbar-left pull-left" role="search" id="l">
              <div class="form-group">
                  <input type="text" class="span7" placeholder="Search a song here" id="pizza"/>
                  <button id="kaka" class="btn btn-success">Search</button>
              
              </div>
          </div>
          <button type="button" class="btn btn-success pull-right" onclick="parent.location='/Home/Index'">Logout</button>
          <button type="button" class="btn btn-success pull-right" onclick="parent.location='/User/ChangeSettings/'">Change Settings</button>
      </div>
  </nav>

    <div class="col-">
        <br />
        <label id="fo">Upload a Song here</label>
      
        <form action="/Home/File" method="post" enctype="multipart/form-data">
            <input type="file" class ="btn btn-success" name="file" /><br />
            <button type="submit" class="btn btn-success">Upload</button>
        </form>
        </div>

        <!-- audio player begin -->
   <br /><br /><br /><br /><br /><br /><br /><br /><br />
       
        <div id="jplayer"></div>
        <div class="jp-audio" >
            <h2>New Song</h2>
            <div class="jp-type-single">
                <div id="jp_interface_1" class="jp-interface">
                    <ul class="jp-controls">
                        <li><a href="#" class="jp-play"></a></li>
                        <li><a href="#" class="jp-pause"></a></li>
                        <li><a href="#" class="jp-prev">Previous Track</a></li>
                        <li><a href="#" class="jp-next">Next Track</a></li>
                        
                    </ul>
                    <div class="jp-progress">
                        <div class="jp-seek-bar">
                            <div class="jp-play-bar"></div>
                        </div>
                    </div>
                    <div class="jp-title"></div>
                </div>
            </div>
        </div>
        </div>
       
        <table id="myTble">
            <tr>
                <td></td>
            </tr>
        </table>
    

</body>
</html>
